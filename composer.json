{
  "name": "easedevs/yii2-turnstile-validator",
  "description": "Yii2 validator for Cloudflare's Turnstile CAPTCHA alternative",
  "keywords": ["yii2", "cloudflare", "turnstile", "captcha", "widget", "validator"],
  "type": "yii2-extension",
  "license": "GPL-3.0-or-later",
  "support": {
    "source": "https://github.com/easedevs/yii2-turnstile-validator",
    "issues": "https://github.com/easedevs/yii2-turnstile-validator/issues"
  },
  "authors": [
    {
      "name": "Ease Tech LLC",
      "email": "support@easedevs.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0",
    "yiisoft/yii2-httpclient": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "easedevs\\yii2\\turnstile\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "easedevs\\yii2\\turnstile\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
